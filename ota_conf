# Common OTA url for all devices
ota_url=https://raw.githubusercontent.com/TurboROM/OTA/master/TurboOTA.xml
release_type=Stable
device_name=ro.turbo.device
 
# Example ro.turbo.version=Turbo-3.3-OFFICIAL-04-13-2016
version_source=ro.turbo.version
version_delimiter=-
version_position=4
version_format=yyyyMMdd
