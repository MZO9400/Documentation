# Common OTA url for all devices
ota_url=https://raw.githubusercontent.com/TurboROM-Devices/OTA/mm/TurboOTA.xml
release_type=Stable
device_name=ro.turbo.device
 
# Example ro.turbo.version=Turbo-3.3-OFFICIAL-20160413
version_source=ro.modversion
